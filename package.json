{
  "name": "electron-react-vite-ts-template",
  "author": "xxx",
  "description": "xxx",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "main": "app/index.mjs",
  "scripts": {
    "downloadCPA": "yarn add download-cli -D && download https://github.com/Need-an-AwP/process-audio-capture-stdio/releases/download/v0.0.2/process-audio-capture.exe --out app/",
    "dev": "concurrently -k \"vite\" \"cross-env DEV=true electron app/index.mjs\"",
    "dev:build": "yarn build:go && yarn dev",
    "build:go": "go build -C twg -o ../app",
    "build:fe": "tsc -b && vite build",
    "build:win": "electron-builder --win",
    "build": "npm run build:fe && npm run build:win",
    "lint": "eslint .",
    "preview": "vite preview",
    "add:shadcn": "npx shadcn@latest add button alert-dialog avatar button card context-menu dialog dropdown-menu input label popover progress resizable scroll-area select separator slider sonner switch tooltip sheet badge tabs"
  },
  "build": {
    "appId": "com.relayx.turn-app",
    "productName": "RelayX",
    "directories": {
      "output": "release",
      "app": "."
    },
    "files": [
      "package.json",
      "dist/**/*",
      "app/**/*"
    ],
    "asarUnpack": [
      "app/tailscale-webrtc-gateway.exe",
      "app/process-audio-capture.exe"
    ],
    "win": {
      "target": [
        "portable"
      ],
      "compression": "maximum"
    },
    "portable": {
      "artifactName": "RelayX-Portable-${version}.${ext}"
    }
  },
  "dependencies": {
